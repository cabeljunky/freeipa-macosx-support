objectClasses: (
    1.3.6.1.4.1.63.1000.1.1.2.1
    NAME 'apple-account'
    SUP top
    AUXILIARY
    DESC 'generic apple account'
    MAY ( authAuthority $
          sambaAcctFlags $
          sambaPwdLastSet $
          sambaLogonTime $
          sambaLogoffTime $
          sambaKickoffTime $
          sambaHomeDrive $
          sambaLogonScript $
          sambaProfilePath $
          sambaUserWorkstations $
          sambaHomePath $
          sambaSID $ s
          sambaPrimaryGroupSID $
          userCertificate $
          userPKCS12 $
          jpegPhoto $
          altSecurityIdentities ) )

